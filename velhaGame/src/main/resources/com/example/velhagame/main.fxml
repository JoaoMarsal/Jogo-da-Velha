<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.velhagame.MenuController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" prefHeight="51.0" prefWidth="600.0" text="Jogo da Velha" textFill="#0d6f00">
               <font>
                  <Font name="Caladea" size="24.0" />
               </font>
            </Label>
            <HBox prefHeight="317.0" prefWidth="600.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="277.0" prefWidth="273.0" spacing="20.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#abrirInicial" text="Iniciar" />
                        <Button mnemonicParsing="false" text="Configurações" onAction="#abrirOpcoes"/>
                        <Button mnemonicParsing="false" text="Créditos" onAction="#abrirCreditos" />
                     </children></VBox>
                  <VBox alignment="CENTER" prefHeight="317.0" prefWidth="309.0" style="-fx-border-color: red; -fx-border-width: 5;">
                     <children>
                        <ImageView fitHeight="317.0" fitWidth="352.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@imagens/velha.png" />
                           </image></ImageView>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
